@model Shopping.Data.Entities.City

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Está seguro de querer eliminar esta ciudad?</h3>
<div>
    <partial name="_CityDetails" />
    <form asp-action="DeleteCity">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-outline-danger" />
        <a class="btn btn-outline-success" asp-action="DetailsState" asp-route-id="@Model?.State.Id">Regresar</a>
    </form>
</div>
